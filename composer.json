    "keywords": ["laravel5.5", "wechat auth login", "微信公众号登录laravel"],
    "homepage": "https://github.com/bluetoothswh/LaravelWeixinLogin.git",
    "license": "MIT",
    "authors": [
        {
            "name": "David Sun",
            "email": "bluetooth_swh@163.com",
            "homepage": "https://larashuo.com",
            "role": "Developer"
        }
